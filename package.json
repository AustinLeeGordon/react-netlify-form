{
  "name": "react-netlify-form",
  "version": "1.0.0",
  "keywords": [
    "react",
    "netlify",
    "form"
  ],
  "main": "dist/index.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/react-netlify-form.git"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "esca-scripts analyze --react --component --input ./src/index.js",
    "build": "esca-scripts build --react --component --babel --input src --output dist --multiple",
    "dev": "esca-scripts dev ---input './dev/dev.js, ./dev/index.html' --react --browser",
    "prepublishOnly": "npm run build",
    "reset": "esca-scripts reset",
    "travis": "opn https://travis-ci.org/escaladesports/react-netlify-form/branches"
  },
  "devDependencies": {
    "esca-scripts": "^0.14.1",
    "opn-cli": "^3.1.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  },
  "dependencies": {
    "fetch-retry": "^1.1.1",
    "form-serialize": "^0.7.2",
    "styled-jsx": "^2.2.4"
  }
}
